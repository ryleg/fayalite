
import org.apache.spark.SparkContext


class Bogus extends Serializable {


}

object Problems {
  var sc: SparkContext = _

  def main() = {

    val randomData = (1 to 1000).map{_ => scala.util.Random.nextInt()}.toArray

    val lookup = randomData.zipWithIndex.map{_.swap}.toMap

    val randomDataRDD = sc.parallelize(randomData).cache().setName("randomData")
    println(randomDataRDD.count())


  }

}